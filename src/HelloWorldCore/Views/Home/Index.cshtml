@model IEnumerable<HelloWorldCore.Models.Pokemon>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Pokemon</title>
</head>
<body>
    <h1>Hello from the html!</h1>

    <table>
        @foreach (var poke in Model)
        {
            <tr>
                <td>@poke.Id</td>
                <td>@poke.Name</td>
            </tr>
        }
    </table>

</body>
</html>